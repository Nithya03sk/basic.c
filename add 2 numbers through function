#include<stdio.h>
 int sum(int n1, int n2) {
   
    n1 + n2;
   return (n1+n2);
}
int main() {
   int n1, n2, res;
 
   printf("\nEnter the two numbers : ");
   scanf("%d %d", &n1, &n2);
 
   
   res = sum(n1, n2);
 
   printf("nAddition of two number is : ");
   printf("%d",res);
   return (0);
}
 
